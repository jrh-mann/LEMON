    "decision_points": [
      {
        "name": "D1_Under_Specialist_Clinic",
        "description": "Checks if patient is already managed by a specialist clinic.",
        "condition": "under_specialist_clinic == true",
        "inputs_required": [
          "under_specialist_clinic"
        ],
        "branches": [
          {
            "condition": "== true",
            "outcome": "No Further Action",
            "leads_to": "OUTPUT_No_Further_Action"
          },
          {
            "condition": "== false",
